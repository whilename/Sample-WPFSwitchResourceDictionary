<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">
    <!-- String resource dictionary -->
    <s:String x:Key="Greeting">你好 WPF!</s:String>
    <s:String x:Key="Language">切换语言</s:String>
    
    <!-- Style and Media Resource Dictionary -->
    <SolidColorBrush x:Key="MyLibSCB" Color="#ee3143"></SolidColorBrush><!-- Background color -->
    <ImageBrush x:Key="MyLibImgB" ImageSource="pack://application:,,,/WpfLibrary;component/Themes/zh-CN.gif"></ImageBrush><!-- Background image -->
    <!-- image source -->
    <BitmapImage x:Key="MyLibImg" UriSource="pack://application:,,,/WpfLibrary;component/Themes/zh-CN.gif" /><!-- Absolute Uri -->
    <BitmapImage x:Key="MyLibBitmap" UriSource="/WpfLibrary;component/Themes/zh-CN.gif"></BitmapImage><!-- Relative Uri -->
    <!-- Style setting -->
    <Style TargetType="Image" x:Key="MyLibStyle">
        <Setter Property="Source" Value="pack://application:,,,/WpfLibrary;component/Themes/zh-CN.gif" />
    </Style>
</ResourceDictionary>
    